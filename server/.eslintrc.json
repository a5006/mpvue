{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": "standard",
  "rules": {
    "indent": [2, 4, {
      "SwitchCase": 1
    }],
    "arrow-parens": 0,
    "generator-star-spacing": 0,
    "space-before-function-paren": ["warn", "never"]
  },
  "env": {
    "mocha": true
  }
}
